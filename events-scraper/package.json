{
  "name": "feecollector-report-scrap",
  "version": "0.0.1",
  "description": "A serverless service that scrapes onchain events generated by the LiFi fee collector contracts on a given blockchain.",
  "author": {
    "name": "Jabba Ã¸1",
    "email": "jabba@srenault.com",
    "url": "https://github.com/ja88a"
  },
  "repository": {
    "url": "https://github.com/ja88a/feecollector-report",
    "type": "git"
  },
  "private": true,
  "license": "AGPL-3.0-only",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "serverless:offline": "serverless offline",
    "start": "DEV_MODE=1 nest start",
    "start:dev": "DEV_MODE=1 LOG_LEVEL=debug nest start --watch",
    "start:debug": "DEV_MODE=1 LOG_LEVEL=debug nest start --debug --watch",
    "start:prod": "DEV_MODE=1 node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@ethersproject/abstract-provider": "^5.7.0",
    "@lifi/types": "^11.4.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/core": "^10.3.3",
    "@typegoose/typegoose": "^12.2.0",
    "aws-lambda": "^1.0.7",
    "ethers": "^5.7.2",
    "feecollector-report-common": "workspace:^",
    "kindagoose": "^1.3.8",
    "lifi-contract-typings": "workspace:^",
    "mongoose": "^8.2.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/config": "^3.2.0",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/aws-lambda": "^8.10.134",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.11.24",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "serverless-offline": "^13.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
